{
    "name": "@fraym/streams",
    "version": "0.10.1",
    "license": "MIT",
    "homepage": "https://github.com/fraym/streams-nodejs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fraym/streams-nodejs.git"
    },
    "description": "nodejs client implementation for our event streaming service",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 0",
        "format": "prettier --write \"**/*.{ts,tsx,json}\"",
        "lint": "prettier --check \"**/*.{ts,tsx,json}\"",
        "build": "npm run clean && tsc",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm test && npm run lint && npm run build",
        "preversion": "npm run lint"
    },
    "files": [
        "dist/**/*"
    ],
    "engines": {
        "node": ">=16"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "dependencies": {
        "@fraym/proto": "^0.20.0",
        "@grpc/grpc-js": "^1.11.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@becklyn/prettier": "^1.0.2",
        "@types/uuid": "^8.3.4",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5"
    },
    "prettier": "@becklyn/prettier"
}
